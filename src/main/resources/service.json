{
  "name": "service1",
  "endpoints": [
    {
      "path": "aggregator",
      "type": "API",
      "dependents": {
        "api1": "localhost:8080/test1",
        "mysql": "localhost:8080/mysql",
        "api2": "localhost:8080/test2"
      },
      "response": {
        "data": {
          "api1": "/time",
          "api2": "/time",
          "mysql": "/mysql"
        }
      }
    },
    {
      "path": "test1",
      "type": "NONE",
      "response": {
        "data": {
          "time": "${{date}}"
        }
      }
    },
    {
      "path": "test2",
      "type": "NONE",
      "response": {
        "data": {
          "time": "${{date}}"
        }
      }
    },
    {
      "path": "mysql",
      "type": "MYSQL",
      "dependents": {
        "mysql": "SELECT 1 as `user_id`,'Happy Singh' as `name`,'Retail' as `Dept` from DUAL;"
      },
      "response": {
        "data": {
          "mysql": ""
        }
      }
    },
    {
      "path": "mysql1",
      "type": "MYSQL",
      "dependents": {
        "mysql": "SELECT CURDATE();"
      },
      "response": {
        "data": {
          "mysql": ""
        }
      }
    },
    {
      "path": "mongo",
      "type": "MONGO",
      "dependents": {
        "mongo": "Date()"
      },
      "response": {
        "data": {
          "mongo": ""
        }
      }
    }
  ]
}